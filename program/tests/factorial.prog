fn factorial(int n) -> int {
  int fnn;
  int fnn1;
  int fnn2;
  set(fnn2, 0);
  set(fnn1, 1);

  if (ieq(n, 2)) {
    return n;
  }
  else {
    int i;

    for i in range(2, n, 1) {
      set(fnn, add(fnn1, fnn2));
      set(fnn2, fnn1);
      set(fnn1, fnn);
    }
    return fnn;
  }
}

fn main() {
  int n;
  read(n);
  print("factorial of n is: ");
  set(n, factorial(n));
  print(n);
  print("\n");
}
